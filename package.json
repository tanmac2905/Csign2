{
  "name": "csign-pdf-view",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--max_old_space_size=8192 & ng serve --open",
    "start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false",
    "start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
    "build-stats": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --stats-json",
    "build:prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production",
    "build:prod-stats": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production --stats-json",
    "bundle-analyzer": "webpack-bundle-analyzer dist/csign/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.3",
    "@angular/cdk": "12.0.4",
    "@angular/common": "12.0.3",
    "@angular/compiler": "12.0.3",
    "@angular/core": "12.0.3",
    "@angular/flex-layout": "12.0.0-beta.34",
    "@angular/forms": "12.0.3",
    "@angular/localize": "^12.2.8",
    "@angular/platform-browser": "12.0.3",
    "@angular/platform-browser-dynamic": "12.0.3",
    "@angular/router": "12.0.3",
    "@ng-bootstrap/ng-bootstrap": "9.1.2",
    "@ng-select/ng-select": "6.1.0",
    "@ngrx/store": "^12.4.0",
    "@ngrx/store-devtools": "^12.4.0",
    "@ngx-translate/core": "13.0.0",
    "@pdftron/webviewer": "^8.1.0",
    "@swimlane/ngx-datatable": "19.0.0",
    "angular-in-memory-web-api": "0.11.0",
    "animate.css": "4.1.1",
    "bootstrap": "4.6.0",
    "bs-stepper": "1.7.0",
    "core-js": "3.14.0",
    "feather-icons": "4.28.0",
    "hammerjs": "2.0.8",
    "katex": "0.13.11",
    "ng-block-ui": "3.0.2",
    "ng2-dragula": "2.1.1",
    "ng2-file-upload": "1.4.0",
    "ng2-pdf-viewer": "^7.0.1",
    "ngx-extended-pdf-viewer": "^10.0.2",
    "ngx-highlightjs": "4.1.4",
    "ngx-perfect-scrollbar": "10.1.1",
    "ngx-plyr": "4.0.0",
    "ngx-quill": "14.0.0",
    "ngx-swiper-wrapper": "10.0.0",
    "ngx-toastr": "14.0.0",
    "node-forge": "^0.10.0",
    "node-waves": "0.7.6",
    "nouislider": "15.1.1",
    "plyr": "3.6.8",
    "quill": "^1.3.7",
    "rxjs": "6.6.0",
    "tslib": "2.2.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.0.3",
    "@angular/cli": "^12.0.4",
    "@angular/compiler-cli": "12.0.3",
    "@angular/language-service": "12.0.3",
    "@angularclass/hmr": "3.0.0",
    "@types/jasmine": "3.6.3",
